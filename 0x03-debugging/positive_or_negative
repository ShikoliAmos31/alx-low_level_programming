#include <stdio.h>

/**
*
*
*/
void positive_or_negative(int i)
{
if (i > 0)
{
printf("%d is positive\n", i);
}
else if (i == 0)
{
printf("%d is zero\n", i);
}
else
{
printf("%d is negative\n", i);
}
}
int main()
{
int number = 42;  // You can replace 42 with any integer you want to test
positive_or_negative(number);
return 0;
}

